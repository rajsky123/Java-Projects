<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:context="http://www.springframework.org/schema/context"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
    http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.1.xsd">

	<context:annotation-config />
	
	<bean id="t" class="beans.Student">
	<property name="rollno" value="123"></property>
	<property name="name" value="jagdev"></property>
	</bean>
	<bean id="pass1" class="beans.Passport" >
		<property name="passportId" value="pass123"></property>
	</bean>

	<bean id="pass2" class="beans.Passport" >
		<property name="passportId" value="pass1234"></property>
	</bean>


</beans>			